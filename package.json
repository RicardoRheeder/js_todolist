{
    "name": "js_todolist",
    "version": "0.0.1",
    "description": "A simple todo list in React",
    "main": "src/app.js",
    "scripts": {
        "start": "webpack-dev-server --config ./build-config/webpack.config.js --watch-poll",
        "build": "webpack -p --config ./build-config/webpack.prod.config.js",
        "deploy": "npm run build && npm run deploy-gh",
        "deploy-gh": "gh-pages -d build"
    },
    "author": "Ricardo Rheeder",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.18.0",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-router-dom": "^5.0.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-es2015-modules-amd": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.16.0",
        "babel-preset-stage-0": "^6.22.0",
        "gh-pages": "^1.2.0",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "webpack": "^4.12.0",
        "webpack-cli": "^3.0.7",
        "webpack-dev-server": "^3.1.4",
        "style-loader": "^0.21.0",
        "css-loader": "^0.28.11"
    }
